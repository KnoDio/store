.mes{
    display: flex;
    width: 100%;
    height: 300rpx;
    background-color: #FFE4E1;
}
.ua{
    width: 150rpx;
    height: 150rpx;
    margin-top: 75rpx;
    margin-left: 70rpx;
    border-radius: 15rpx;
    overflow: hidden;
}
.un{
    height: 60rpx;
    width: 25%;
    font-size: 40rpx;
    margin-top: 75rpx;
    margin-left: 35rpx;
}
.title{ 
    display: flex;
    width: 90%;
    margin-left: 30rpx;
    height: 100rpx;
    background-color: white;
    align-items: center;
}
.title text{
    width: 200rpx;
    margin-left: 25rpx;
